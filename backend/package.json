{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "build-server": "npm run build-ts && npm run tslint && npm run cp-env",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "cp-env": "cp .env-production dist/.env -f",
    "serve-debug": "nodemon --inspect dist/server/server.js",
    "build": "npm run build-server && concurrently \"npm run serve\" ",
    "start": "nodemon --exec ts-node server.ts",
    "start_sensors": "nodemon --exec ts-node start_sensors.ts",
    "dev": "nodemon server.ts",
    "prestart2": "npx sequelize db:create --config=./sequelize/src/config/config.js",
    "serve": "cd dist && node server.js",
    "db:reset": "ts-node src/models/Mongo_reset.ts",
    "mongoexport": "mongoexport -d shop -c questions --jsonArray --pretty -o db/questions.json & mongoexport -d shop -c users --jsonArray --pretty -o db/users.json & mongoexport -d shop -c articles --jsonArray --pretty -o db/articles.json & mongoexport -d shop -c products --jsonArray --pretty -o db/products.json",
    "mongoimport": " mongoimport -d shop -c questions --jsonArray --file db/questions.json & mongoimport -d shop -c users --jsonArray --file db/users.json &  mongoimport -d shop -c articles --jsonArray --file db/articles.json &  mongoimport -d shop -c products --jsonArray --file db/products.json"
  },
  "keywords": [
    "node",
    "ts",
    "tslint",
    "yarn"
  ],
  "author": "Ali",
  "license": "ISC",
  "dependencies": {
    "@types/express-fileupload": "^1.1.6",
    "axios": "^0.20.0",
    "bcrypt": ">=5.0.0",
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-fileupload": "^1.2.1",
    "express-graphql": "^0.11.0",
    "express-session": "^1.17.1",
    "fs": "0.0.1-security",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "mongodb": "^3.5.9",
    "mongoose": "^5.9.20",
    "mysql2": "^2.1.0",
    "node-input-validator": "^4.2.1",
    "node-tsc": "0.0.14",
    "sequelize": "^5.21.13",
    "socket.io": "^2.3.0",
    "ts-node": "^8.10.2",
    "tsc": "^1.20150623.0",
    "tsc-node": "0.0.3",
    "tslint": "^6.1.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.0.4",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.17.6",
    "@types/express-ejs-layouts": "^2.3.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.5.25",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@types/sequelize": "^4.28.9",
    "@types/socket.io": "^2.1.11",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.0.0",
    "bl": ">=2.2.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.2",
    "helmet": "^4.1.1",
    "jest": "^26.1.0",
    "mocha": "^8.0.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "sequelize-cli": "^5.5.1",
    "supertest": "^4.0.2",
    "typescript": "^3.9.5"
  },
  "_moduleAliases": {
    "@sequelize": "sequelize/src"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
