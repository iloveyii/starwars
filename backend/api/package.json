    "apollo-server": "^2.21.0",
    "apollo-server-testing": "^2.21.0",
    "axios": "^0.19.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "esm": "^3.2.25",
    "express": "~4.16.0",
    "express-pino-logger": "^4.0.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "http-errors": "~1.6.2",
    "node-fetch": "^2.6.1",
    "pino": "^5.16.0",
    "pino-pretty": "^3.5.0",
