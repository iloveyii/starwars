{
  "name": "react-mui",
  "version": "1.0.0",
  "description": "A MERN application with webpack and material-ui",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --port 9000 --host 0.0.0.0  --compress --hot",
    "_build:dev": "webpack --mode development --progress",
    "_build:prod": "webpack --mode production --progress",
    "build": "npm run _build:prod",
    "post_build:dev2": "cp public/manifest.json dist/ & cp public/images dist/ -Rf",
    "post_build:prod": "cp public/images dist/ -Rf && cp public/css dist/ -Rf && cp public/js dist/ -Rf",
    "serve": "serve -s dist",
    "browserslist": "browserslist 'last 1 version, >1%' "
  },
  "keywords": [
    "node",
    "webpack",
    "react",
    "material-ui"
  ],
  "author": "Hazrat Ali <iloveyii@yahoo.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "@date-io/date-fns": "^1.3.13",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "4.11.0",
    "@material-ui/data-grid": "^4.0.0-alpha.9",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.20.0",
    "bson": "^4.1.0",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.20",
    "notistack": "^1.0.0",
    "react": "^16.13.1",
    "react-barcode": "^1.4.0",
    "react-color": "^2.19.3",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "shortid": "^2.2.15",
    "socket.io-client": "^2.3.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "browserslist": "^4.14.5",
    "css-loader": "^4.3.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.5.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
